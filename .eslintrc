{
	"extends": ["next", "next/core-web-vitals", "prettier", "eslint:recommended"],
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"no-console": "warn",
		"no-alert": "warn",
		"eqeqeq": "error",
		"prefer-const": "error",
		"no-use-before-define": "error",
		"react-hooks/exhaustive-deps": "off"
	}
}
